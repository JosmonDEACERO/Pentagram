<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Pentagram" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Primary_color</item>
        <item name="colorPrimaryDark">@color/Dark_primary_color</item>
        <item name="colorAccent">@color/Accent_color</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Pentagram.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Pentagram.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Pentagram.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>